{
  "name": "pharos",
  "version": "3.12.0",
  "tcrd_version": "6.12.4",
  "license": "MIT",
  "scripts": {
    "deploy": "GAE_VERSION=$(echo ${npm_package_version} | sed 's/\\./-/g') && gcloud app deploy --version $GAE_VERSION",
    "ng": "ng",
    "start": "node dist/server/main.js",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build pharos --configuration production",
    "test": "ng test pharos --code-coverage",
    "lint": "ng lint pharos",
    "lintfix": "ng lint pharos --fix",
    "e2e": "ng e2e",
    "build:appshell": "ng run pharos:app-shell:production",
    "compodoc": "npx compodoc -p tsconfig.app.json -s -w --theme material",
    "build:ssr": "npm --no-git-tag-version version patch && npm run prerender",
    "build:ssr:dev": "ng build && ng run pharos:server:development",
    "serve:ssr": "node dist/server/main.js",
    "perfmon": "node dist/server/main.js perf",
    "rerun:ssr": "npm run build:ssr && say pharos is ready && npm run serve:ssr",
    "dev:ssr": "ng run pharos:serve-ssr",
    "prerender": "ng run pharos:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "~13.1.0",
    "@angular/animations": "~13.3.5",
    "@angular/cdk": "~13.3.5",
    "@angular/common": "~13.3.5",
    "@angular/compiler": "~13.3.5",
    "@angular/core": "~13.3.5",
    "@angular/fire": "~7.3.0",
    "@angular/flex-layout": "~13.0.0-beta.38",
    "@angular/forms": "~13.3.5",
    "@angular/material": "~13.3.5",
    "@angular/platform-browser": "~13.3.5",
    "@angular/platform-browser-dynamic": "~13.3.5",
    "@angular/platform-server": "~13.3.5",
    "@angular/router": "~13.3.5",
    "@angular/service-worker": "~13.3.5",
    "@apollo/client": "~3.6.2",
    "@nguniversal/express-engine": "~13.1.0",
    "angular-shepherd": "~13.0.1",
    "apollo-angular": "~3.0.1",
    "d3": "npm:d3@^5.X",
    "d3v7": "npm:d3@^7.1.1",
    "express": "~4.18.1",
    "file-saver": "~2.0.5",
    "firebase": "~9.7.0",
    "graphql": "~16.4.0",
    "json2csv": "~5.0.7",
    "jszip": "~3.9.1",
    "ncats-protvista-uniprot": "~2.8.39",
