{
  "name": "pharos",
  "version": "3.18.0",
  "tcrd_version": "6.13.4",
  "license": "MIT",
  "scripts": {
    "deploy": "GAE_VERSION=$(echo ${npm_package_version} | sed 's/\\./-/g') && gcloud app deploy --version $GAE_VERSION --project=pharos-frontend-dev",
    "ng": "ng",
    "start": "node dist/server/main.js",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build pharos --configuration production",
    "test": "ng test pharos --code-coverage",
    "lint": "ng lint pharos",
    "lintfix": "ng lint pharos --fix",
    "postinstall": "patch-package",
    "e2e": "ng e2e",
    "build:appshell": "ng run pharos:app-shell:production",
    "compodoc": "npx compodoc -p tsconfig.app.json -s -w --theme material",
    "build:ssr": "npm --no-git-tag-version version patch && npm run prerender",
    "build:ssr:dev": "ng build && ng run pharos:server:development",
    "serve:ssr": "node dist/server/main.js",
    "perfmon": "node dist/server/main.js perf",
    "rerun:ssr": "npm run build:ssr && say pharos is ready && npm run serve:ssr",
    "dev:ssr": "ng run pharos:serve-ssr",
    "prerender": "ng run pharos:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "~15.0.0",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/fire": "7.6.1",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@apollo/client": "~3.6.2",
    "@apollo/sandbox": "^1.0.3",
    "@nguniversal/express-engine": "15.2.1",
    "angular-d3-cloud": "^1.1.2",
    "angular-shepherd": "~15.0.0",
    "apollo-angular": "4.2.1",
    "d3": "npm:d3@^5.X",
    "express": "~4.18.1",
    "file-saver": "~2.0.5",
    "firebase": "10.8.0",
    "graphql": "~16.4.0",
    "json2csv": "~5.0.7",
    "jszip": "~3.9.1",
    "ngx-json-viewer": "~3.0.2",
    "ngx-markdown": "~15.1.2",
    "patch-package": "^6.4.7",
    "raw-loader": "~4.0.2",
    "rxfire": "~6.0.3",
    "rxjs": "~6.6.0",
    "sql-formatter": "~4.0.2",
    "svg-inline-loader": "~0.8.2",
    "tslib": "~2.4.0",
    "uuid": "~8.3.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1502.10",
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "~4.17.13",
    "@types/jasmine": "~3.6.0",
    "@types/node": "~17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "firebase-tools": "~10.7.2",
    "fuzzy": "~0.1.3",
