{
  "name": "pharos",
  "version": "3.4.2",
  "tcrd_version": "6.8.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/main.js",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build pharos --prod",
    "postbuild": "node ./post_process_build_dir.js",
    "test": "ng test pharos --code-coverage",
    "lint": "ng lint pharos",
    "lintfix": "ng lint pharos --fix",
    "e2e": "ng e2e",
    "build:appshell": "ng run pharos:app-shell:production",
    "compodoc": "npx compodoc -p tsconfig.app.json -s -w --theme material",
    "build:ssr": "npm --no-git-tag-version version patch && ng build --prod && node ./post_process_build_dir.js && ng run pharos:server:production",
    "serve:ssr": "node dist/server/main.js",
    "rerun:ssr": "npm run build:ssr --prod && say pharos is ready && npm run serve:ssr",
    "dev:ssr": "ng run pharos:serve-ssr",
    "prerender": "ng run pharos:prerender:fix-exit && ng run pharos:prerender && node ./post_process_build_dir.js",
    "prerender:fix-exit": "sed -i -e \"s/}))().then().catch();/}))().then(()=>process.exit(0)).catch(()=>process.exit(1));/g\" ./node_modules/@nguniversal/builders/src/prerender/render.js"
  },
  "private": true,
